{
    "AWSEBDockerrunVersion": 1,
    "containerDefinitions": [ 
        {
            "name": "my-django-app-container",
            "image": "nahlooobi/xtremestoreapp:latest",
            "essential": true,
            "memory": 512,
            "portMappings": [
                {
                    "hostPort": 80, 
                    "containerPort": 8000
                }
            ], "environment": [
                 {
                    "name": "DJANGO_SETTINGS_MODULE", 
                    "value": "ecommerce.settings"
                 } 
                ]
        }
    ]
}